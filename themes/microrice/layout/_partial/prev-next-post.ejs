<p name="pagination" style="font-size: 1.2em">
    <% if (page.next){ %>
    <a class="float-left" href="<%= config.root %><%- page.next.path %>">« <%- page.next.title %></a>
    <% } %>
    <% if (page.prev){ %>
    <a class="float-right" href="<%= config.root %><%- page.prev.path %>">» <%- page.prev.title %></a>
    <% } %>
</p>